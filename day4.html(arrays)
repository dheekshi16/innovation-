            object 
        let detail={
            name:"dheeks",
            age:"19",
            //nested obj
        sub: {
            tamil: 110,
            english:1110
        },
        avg: function() {
            let sum = this.sub.tamil + this.sub.english;
            return sum 
        }
    }
        console.log(detail.age);
        console.log(detail["name"]);
        console.log(detail.sub.tamil);
        console.log(detail.sub.english);
       // key and value
    const person={
